{
  "name": "lint-config-m",
  "version": "3.1.0",
  "description": "",
  "scripts": {
    "test": "ava",
    "test:watch": "npm test -- --watch",
    "test:coverage": "nyc npm run test",
    "coverage": "nyc report --reporter=lcov && codecov",
    "lint:js": "eslint --ignore-path .gitignore .",
    "lint": "npm run lint:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evenchange4/lint-config-m.git"
  },
  "author": "Michael Hsu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evenchange4/lint-config-m/issues"
  },
  "homepage": "https://github.com/evenchange4/lint-config-m#readme",
  "peerDependencies": {
    "eslint": "^3.13.0",
    "stylelint": "^7.7.1"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "codecov": "^1.0.1",
    "eslint": "^3.13.0",
    "nyc": "^10.0.0",
    "stylelint": "^7.7.1"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "failFast": true,
    "verbose": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "exclude": [
      "test/**"
    ],
    "include": [
      "*.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "./eslint.js"
    ]
  },
  "dependencies": {
    "babel-eslint": "^7.1.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-ava": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "stylelint-config-suitcss": "^9.0.0"
  }
}
