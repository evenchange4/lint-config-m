{
  "name": "lint-config-m",
  "version": "1.0.5",
  "description": "",
  "scripts": {
    "test": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evenchange4/lint-config-m.git"
  },
  "author": "Michael Hsu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evenchange4/lint-config-m/issues"
  },
  "homepage": "https://github.com/evenchange4/lint-config-m#readme",
  "scripts": {
    "test": "ava",
    "test:watch": "npm test -- --watch",
    "test:coverage": "nyc npm run test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint:js": "eslint --ignore-path .gitignore .",
    "lint": "npm run lint:js"
  },
  "peerDependencies": {
    "babel-eslint": "^6.0.0-beta.6",
    "eslint": "^2.5.1",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-ava": "^1.3.1",
    "eslint-plugin-react": "^4.2.3",
    "stylelint": "^5.2.1",
    "stylelint-config-suitcss": "^5.0.0"
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "coveralls": "^2.11.8",
    "eslint": "^2.5.1",
    "nyc": "^6.1.1"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "failFast": true,
    "verbose": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "exclude": [
      "test/**"
    ],
    "include": [
      "*.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "./eslint.js"
    ]
  }
}
